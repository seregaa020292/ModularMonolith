DOCKER_BUILDKIT=1
PROJECT_NAME=modular-monolith

# Окружение: development|production
ENV=development

APP_NAME=Fines payment systems
APP_PORT=8080

CORS_ORIGINS=

# func formatter: text|json|pretty
LOG_FORMATTER=pretty
# level: debug|info|warn|error
LOG_LEVEL=debug

PG_DB=app
PG_USER=user
PG_PASSWORD=password
PG_HOST=app-pg
PG_PORT=5432
PG_DSN="dbname=${PG_DB} user=${PG_USER} password=${PG_PASSWORD} host=${PG_HOST} port=${PG_PORT} sslmode=disable"
