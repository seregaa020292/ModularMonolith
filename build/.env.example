DOCKER_BUILDKIT=1

# Окружение: development|production
ENV=development

APP_NAME=Fines payment systems
APP_PORT=8080

CORS_ORIGINS=

# func formatter: text|json
LOG_FORMATTER=text
# level: debug|info|warn|error
LOG_LEVEL=debug

PG_DB=app
PG_USER=user
PG_PASSWORD=password
PG_HOST=app-pg
PG_PORT=54321
PG_DSN="dbname=${PG_DB} user=${PG_USER} password=${PG_PASSWORD} host=localhost port=${PG_PORT} sslmode=disable"
